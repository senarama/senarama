{
  "editor.tabSize": 2,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "node_modules": true,
    ".yarn": true,
    ".editorconfig": true,
  },
  "path-intellisense.mappings": {
    "@data": "${worspaceFolder}/src/client/assets/data",
    "@images": "${workspaceFolder}/src/client/assets/images",
    "@icons": "${workspaceFolder}/src/client/assets/icons",
    "@assets": "${workspaceFolder}/src/client/assets",
    "@components": "${workspaceFolder}/src/client/components",
    "@hooks": "${workspaceFolder}/src/client/hooks",
    "@utils": "${workspaceFolder}/src/client/utils",
    "@store": "${workspaceFolder}/src/client/store",
    "@views": "${workspaceFolder}/src/client/views",
  },
  "files.eol": "\n",
  "javascript.validate.enable": false,
  "javascript.preferences.quoteStyle": "single",
  "material-icon-theme.activeIconPack": "react_redux",
}